replicaCount: 1

mode:
  standalone:
    enabled: true
  distributed:
    enabled: false

ingress:
  className: "nginx"
  hosts:
    - host: "rustfs.profidev.io"
      paths:
        - path: "/"
          pathType: ImplementationSpecific
  tls:
    - secretName: rustfs-tls
      hosts:
        - "rustfs.profidev.io"

storageclass:
  name: ${storage_class}
  size: 20Gi
