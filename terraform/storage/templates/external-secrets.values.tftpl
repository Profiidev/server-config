serviceMonitor:
  enabled: true
  interval: 60s
webhook:
  metrics:
    enabled: true
certController:
  metrics:
    enabled: true
extraVolumeMounts:
  - name: kube-root-ca-crt
    mountPath: /etc/ssl/certs/ca.crt
    subPath: ca.crt
extraVolumes:
  - name: kube-root-ca-crt
    configMap:
      name: kube-root-ca.crt
